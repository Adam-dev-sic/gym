generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model workouts {
  id          Int      @id @default(autoincrement())
  programname String
  workoutname String[]
  workoutgif  String[]
}

model foods {
  id              Int      @id @default(autoincrement())
  foodprogramname String
  foodname        String[]
  foodpic         String[]
  foodcalories    Int[]
  foodprotein     Int[]
  foodquantity    String[]
}
